{
    "name": "phutureproof/eloquence-mappable",
    "version": "1.0.0",
    "description": "Flexible Searchable, Mappable, Metable, Validation and more extensions for Laravel Eloquent ORM.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "eloquent",
        "metable",
        "searchable",
        "mappable",
        "mutable"
    ],
    "authors": [
        {
            "name": "Jarek Tkaczyk",
            "email": "jarek@softonsofa.com",
            "homepage": "https://softonsofa.com/",
            "role": "Developer"
        },
        {
            "name": "Dale Paget",
            "email": "dale.phutureproof@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "phutureproof/eloquence-base": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "2.3.3",
        "mockery/mockery": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Sofa\\Eloquence\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sofa\\Eloquence\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit && ./vendor/bin/phpcs src --standard=psr2 --report=diff --colors",
        "phpcs": "./vendor/bin/phpcs src --standard=psr2 --report=diff --colors"
    }
}
